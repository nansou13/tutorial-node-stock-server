{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-intl": "^3.1.13",
    "react-intl.macro": "^0.3.7",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "REACT_APP_ENV_TYPE=development react-scripts start",
    "start:preprod": "REACT_APP_ENV_TYPE=preprod react-scripts start",
    "start:prod": "REACT_APP_ENV_TYPE=production react-scripts start",
    "postinstall": "rimraf node_modules/jest-runtime/node_modules/babel-core node_modules/jest-config/node_modules/babel-core",
    "test": "jest --json --outputFile=jest/test-results-for-storybook.json",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js",
    "lint:staged": "lint-staged",
    "start:mock": "node src/utils/mock/server.js",
    "start:all": "run-p start:mock start",
    "cypress": "cypress open",
    "i18n:extract": "MESSAGE_DIR='./.messages' react-scripts build",
    "i18n:manageTranslations": "node ./translationRunner.js",
    "start:i18n": "npm run i18n:extract && npm run i18n:manageTranslations"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js"
    ],
    "notify": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname",
      [
        "jest-watch-toggle-config",
        {
          "setting": "verbose"
        }
      ],
      [
        "jest-watch-toggle-config",
        {
          "setting": "collectCoverage"
        }
      ]
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "react-app"
    ],
    "plugins": [
      "macros",
      [
        "styled-components",
        {
          "displayName": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-jest-diff": "^1.0.2",
    "chai-jest-snapshot": "^2.0.0",
    "cors": "^2.8.5",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "jest": "24.8.0",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^9.2.5",
    "lowdb": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "promise": "^8.0.3",
    "react-intl-translations-manager": "^5.0.3",
    "react-test-renderer": "^16.9.0",
    "redux-devtools": "^3.5.0",
    "sinon-chai": "^3.3.0"
  }
}
